<div class="container">
  <div>Name: <%= user.name %></div>
  <div>Email: <%= user.email %></div>
  <br>
  <div><a href="/users/<%= user.id %>/edit">Edit Account</a></div>
  <br>
  <% if surveys %>
    <% surveys.each do |survey| %>
      <%= erb :'/surveys/_show-single-survey', locals:{survey: survey} %>
    <% end %>
  <% end %>
</div>