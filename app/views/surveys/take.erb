<div class='single-survey' id="<%= survey.id %>">
  <div> <%= survey.title %> </div>
  <div class="all_questions">
    <div>
      <% if survey.questions %>
        <% survey.questions.each do |q|  %>
          <%= erb :'questions/_show_single_questions_take', locals: {question: q, survey: survey}  %>
        <% end %>
      <% end %>
    </div>
  </div>
  <br>
  <br>
  <br>
</div>